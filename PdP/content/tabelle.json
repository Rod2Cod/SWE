  "rischi": {
    "interni": {
      "keys": ["Codice", "Rischio", "Descrizione", "Mitigazione"],
      "values": [
        {
          "Codice": "RI-1",
          "Rischio": "Divergenze interne",
          "Descrizione": "Ci potrebbero essere divergenze di pensiero tra i vari membri del gruppo che potrebbero portare a discussioni interne.",
          "Mitigazione": "Se, dopo una discussione collettiva, dovessero presentarsi ancora dei conflitti sarà compito del responsabile risolvere la controversia, principalmente ricorrendo all’utilizzo di sondaggi oppure contattando il docente o il proponente in base all’argomento di discussione."
        },
        {
          "Codice": "RI-2",
          "Rischio": "Ritiro dal progetto",
          "__comment": "Non particolarmente presente nel nostro caso",
          "Descrizione": "Un membro del gruppo, per motivazioni personali, potrebbe decidere di ritirarsi dal progetto.",
          "Mitigazione": "Si cercherà di coprire il lavoro da svolgere, con un membro in meno, negoziando con il proponente sulle funzioni prioritarie da realizzare in modo da non superare il monte ore personale. Il responsabile pianificherà nuovamente le attività da completare."
        },
        {
          "Codice": "RI-3",
          "Rischio": "Disparità di conoscenze",
          "Descrizione": "Ci potrebbe essere una disparità di conoscenze all’interno del gruppo.",
          "Mitigazione": "I membri del team con una maggiore conoscenza dell’argomento aiuteranno gli altri membri nel colmare tale differenza in modo da ridurre le tempistiche di autoapprendimento. Questo verrà effettuato tramite insegnamento diretto o scrittura di manuali ad uso interno."
        },
        {
          "Codice": "RI-4",
          "Rischio": "Disomogeneità della documentazione",
          "Descrizione": "Si potrebbe presentare una disomogeneità nella redazione della documentazione a causa di un`errata scrittura o un mancato aggiornamento del Way of Working.",
          "Mitigazione": "Verrà fatto presente al responsabile e si cercherà di definire il prima possibile delle Norme di Progetto fisse ed evitare cambiamenti frequenti di queste ultime."
        }
      ]
    },
    "tecnologici": {
      "keys": ["Codice", "Rischio", "Descrizione", "Mitigazione"],
      "values": [
        {
          "Codice": "RT-1",
          "Rischio": "Inesperienza con gli strumenti di lavoro",
          "Descrizione": "Si potrebbero presentare difficoltà nell'utilizzo degli strumenti di lavoro.",
          "Mitigazione": "Si può utilizzare del tempo per approfondire in autonomia l'utilizzo di questi strumenti o eventualmente richiedere un colloquio con il proponente al fine di coprire le difficoltà del gruppo in merito ad uno specifico argomento."
        },
        {
          "Codice": "RT-2",
          "Rischio": "Guasti hardware/software",
          "Descrizione": "Si potrebbero presentare problemi di natura hardware o software per cui un membro del gruppo potrebbe avere difficoltà a portare a termine un obiettivo.",
          "Mitigazione": "Si avviserà il resto del gruppo del guasto e si provvederà a svolgere il proprio lavoro tramite altri mezzi."
        }
      ]
    },
    "organizzativi": {
      "keys": ["Codice", "Rischio", "Descrizione", "Mitigazione"],
      "values": [
        {
          "Codice": "RO-1",
          "Rischio": "Ritardo di consegna",
          "Descrizione": "Un membro del gruppo potrebbe essere in ritardo con una consegna a causa di una sottostima del carico di lavoro dell’impegno preso oppure per un malessere fisico.",
          "Mitigazione": "Si concederà più tempo per quel particolare compito. Nel caso in cui dovesse essere necessario a causa di tempistiche strette, verrà affiancato da uno o più membri del gruppo, che si potrebbero sostituire al membro designato inizialmente nel caso in cui fosse impossibilitato a svolgere il suo lavoro."
        },
        {
          "Codice": "RO-2",
          "Rischio": "Stima errata dei costi",
          "Descrizione": "Potrebbe verificarsi un'errata stima dei costi per un incarico.",
          "Mitigazione": "Si fisserà un incontro interno con il gruppo per discutere di un eventuale ricalcolo dei costi di quella particolare attività ed eventuali successive.",
          "Mitigazione_old": "Si prevede di utilizzare un diagramma di Gantt per organizzare le attività e si cercherà di lasciare un margine temporale abbondante tra le attività con dipendenze."
        }
      ]
    },
    "capitolato": {
      "keys": ["Codice", "Rischio", "Descrizione", "Mitigazione"],
      "values": [
        {
          "Codice": "RC-1",
          "Rischio": "Carico di lavoro eccessivo",
          "Descrizione": "Ci si potrebbe rendere conto, a progetto in corso, che il carico di lavoro del capitolato che si è deciso di prendere in carico potrebbe essere eccessivo.",
          "Mitigazione": "Si fisserà il prima possibile un incontro con il proponente dove si andrà a negoziare su quali siano gli aspetti fondamentali del progetto e quali aspetti possano essere tralasciati; il tutto in modo da rispettare il vincolo imposto sul totale delle ore produttive assegnate ad ogni membro del gruppo."
        },
        {
          "Codice": "RC-2",
          "Rischio": "Incomprensione dei requisiti",
          "Descrizione": "Durante l’analisi dei requisiti ci potrebbero essere errori dovuti ad un errata interpretazione/scrittura dei requisiti da parte degli analisti.",
          "Mitigazione": "Si cercherà di fissare il prima possibile un incontro con il proponente in modo da assicurare la corretta comprensione dei requisiti espressi nella presentazione del capitolato."
        },
        {
          "Codice": "RC-3",
          "Rischio": "Modifica dei requisiti",
          "Descrizione": "Durante il progetto ci potrebbero essere dei cambiamenti dei requisiti dal lato del proponente.",
          "Mitigazione": "Si cercherà di adeguarsi a tali cambiamenti capendo quali aggiustamenti fare in corso d’opera, eventualmente cercando di arrivare ad un compromesso con il proponente."
        }
      ]
    },
